{
"manifest_version": 2,
    "name": "Bitcoin to Bullshit",
    "description": "Chrome extension that replaces 'bitcoin' with 'bullshit'.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
