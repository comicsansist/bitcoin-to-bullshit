{
"manifest_version": 2,
    "name": "Bitcoin to Bullshit",
    "description": "This Chrome extension replaces 'Bitcoin' with 'Bullshit'.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}